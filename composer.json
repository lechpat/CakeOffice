{
    "name": "lechpat/cake-office",
    "description": "MS Office plugin for CakePHP",
    "type": "cakephp-plugin",
    "homepage": "https://github.com/lechpat/CakeOffice",
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Valdez",
            "email": "lechpat@me.com"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "phpoffice/phpexcel": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakeOffice\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeOffice\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
